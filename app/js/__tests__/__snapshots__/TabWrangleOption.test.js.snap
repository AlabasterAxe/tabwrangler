exports[`should render options with exactURLMatch selected 1`] = `
<div>
  <label
    htmlFor="wrangleOption"
  >
    Prevent duplicate tabs in the Tab Corral:
  </label>
  <div
    className="radio"
  >
    <label>
      <input
        checked={false}
        className="form-check-input"
        id="withDupes"
        name="wrangleOption"
        onChange={[Function]}
        type="radio"
        value="withDupes"
      />
       
      Never (Default)
    </label>
  </div>
  <div
    className="radio"
  >
    <label>
      <input
        checked={true}
        className="form-check-input"
        id="exactURLMatch"
        name="wrangleOption"
        onChange={[Function]}
        type="radio"
        value="exactURLMatch"
      />
       
      If URL matches exactly
    </label>
  </div>
  <div
    className="radio"
  >
    <label>
      <input
        checked={false}
        className="form-check-input"
        id="hostnameAndTitleMatch"
        name="wrangleOption"
        onChange={[Function]}
        type="radio"
        value="hostnameAndTitleMatch"
      />
       
      If hostname and page title match
    </label>
  </div>
  <div
    className="row"
  >
    <div
      className="col-xs-8 help-block"
    >
      <dl>
        <dt>
          Never
        </dt>
        <dd>
          When a tab is wrangled, it will be added to the Corral. No other changes will be made to the Corral.
        </dd>
        <dt
          style={
            Object {
              "marginTop": "10px",
            }
          }
        >
          If URL matches exactly
        </dt>
        <dd>
          When a tab is wrangled, it will be added to the Corral and any older tabs with the exact same URL will be removed from the Corral.
        </dd>
        <dt
          style={
            Object {
              "marginTop": "10px",
            }
          }
        >
          If hostname and page title match
        </dt>
        <dd>
          When a tab is wrangled, it will be added to the Corral and any older tabs with the same hostname (like "wikipedia.org") and page title will be removed from the Corral.
        </dd>
      </dl>
    </div>
  </div>
</div>
`;

